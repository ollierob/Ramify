            <xs:element ref="tns:church"/>
                        
            <xs:element ref="tns:monastery"/>
                        
            <xs:element ref="tns:mill"/>
                        
            <xs:element ref="tns:school"/>
                        
            <xs:element ref="tns:inn"/>
                        
            <xs:element ref="tns:graveyard"/>
                      
          </xs:choice>
                  
        </xs:sequence>
              
      </xs:extension>
          
