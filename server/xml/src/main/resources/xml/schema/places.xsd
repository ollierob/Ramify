            <xs:element ref="ns1:builtInYear"/>
                        
            <xs:element ref="ns1:builtBetwweenYears"/>
                        
            <xs:element ref="ns1:builtBeforeYear"/>
                      
          </xs:choice>
                  
        </xs:sequence>
                
        <xs:attribute name="countryIso" type="xs:string"/>
                
        <xs:attribute name="demolished" type="xs:boolean"/>
              
      </xs:extension>
          
    </xs:complexContent>
      
  </xs:complexType>
    
  <xs:complexType name="xmlMill">
        
    <xs:complexContent>
            
      <xs:extension base="tns:xmlPlace">
                
        <xs:sequence>
                    
          <xs:choice minOccurs="0">
                        
            <xs:element ref="ns1:builtInYear"/>
                        
            <xs:element ref="ns1:builtBetwweenYears"/>
                        
            <xs:element ref="ns1:builtBeforeYear"/>
                      
          </xs:choice>
                  
        </xs:sequence>
                
        <xs:attribute name="countryIso" type="xs:string"/>
                
        <xs:attribute name="demolished" type="xs:boolean"/>
              
      </xs:extension>
          
    </xs:complexContent>
      
  </xs:complexType>
    
  <xs:complexType name="xmlSchool">
        
    <xs:complexContent>
            
      <xs:extension base="tns:xmlPlace">
                
        <xs:sequence>
                    
          <xs:choice minOccurs="0">
                        
            <xs:element ref="ns1:builtInYear"/>
                        
            <xs:element ref="ns1:builtBetwweenYears"/>
                        
            <xs:element ref="ns1:builtBeforeYear"/>
                      
          </xs:choice>
                  
        </xs:sequence>
                
        <xs:attribute name="countryIso" type="xs:string"/>
                
        <xs:attribute name="demolished" type="xs:boolean"/>
              
      </xs:extension>
          
    </xs:complexContent>
      
  </xs:complexType>
    
  <xs:complexType name="xmlInn">
        
    <xs:complexContent>
            
      <xs:extension base="tns:xmlPlace">
                
        <xs:sequence>
                    
          <xs:choice minOccurs="0">
                        
            <xs:element ref="ns1:builtInYear"/>
                        
            <xs:element ref="ns1:builtBetwweenYears"/>
                        
            <xs:element ref="ns1:builtBeforeYear"/>
                      
          </xs:choice>
                  
        </xs:sequence>
                
        <xs:attribute name="countryIso" type="xs:string"/>
                
        <xs:attribute name="demolished" type="xs:boolean"/>
              
      </xs:extension>
          
    </xs:complexContent>
      
  </xs:complexType>
    
  <xs:complexType name="xmlTown">
        
    <xs:complexContent>
            
      <xs:extension base="tns:xmlSettlement">
                
        <xs:sequence>
                    
          <xs:choice maxOccurs="unbounded" minOccurs="0">
                        
