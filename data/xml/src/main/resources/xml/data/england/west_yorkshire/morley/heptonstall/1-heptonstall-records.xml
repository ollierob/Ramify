<?xml version="1.0" encoding="UTF-8" ?>

<recordSets
        xmlns="http://ramify.net/records"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:d="http://ramify.net/dates"
        xsi:schemaLocation="http://ramify.net/records ../../../../../schema/records.xsd http://ramify.net/dates ../../../../../schema/dates.xsd">

    <recordSet
            id="heptonstall_st-thomas_registers"
            parentId="wry_coe_registers"
            title="Heptonstall St. Thomas Registers"
            place="chapelry:heptonstall"
            source="church"
            type="mixed">

        <description>
            Registers of the church of St Thomas at Heptonstall.
        </description>

        <d:betweenYears start="1593" end="1986"/>

        <recordSet
                id="heptonstall_st-thomas_baptisms"
                title="Heptonstall St. Thomas Baptisms"
                place="chapelry:heptonstall"
                source="church"
                type="baptism">
            <d:betweenYears start="1599" end="1904"/>
        </recordSet>

        <recordSet
                id="heptonstall_st-thomas_marriages"
                title="Heptonstall St. Thomas Marriages"
                place="chapelry:heptonstall"
                source="church"
                type="marriage">
            <d:betweenYears start="1593" end="1986"/>
        </recordSet>

        <recordSet
                id="heptonstall_st-thomas_pew_rents"
                title="Heptonstall St. Thomas Pew Rentals"
                shortTitle="Pew Rentals"
                place="chapelry:heptonstall"
                source="church"
                type="membership">
            <d:betweenYears start="1796" end="1847"/>
